{
  "name": "web-vault",
  "version": "0.0.1",
  "description": "library for e2e encrypted web vaults, using native web crypto api, written in typescript, easy to use.",
  "private": true,
  "type": "module",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --runInBand --verbose --detectOpenHandles --forceExit",
    "audit": "better-npm-audit audit",
    "qa": "npm run lint && npm run audit && npm run test",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.26.0",
    "@types/argon2-browser": "^1.18.4",
    "@types/jest": "29.5.13",
    "@typescript-eslint/eslint-plugin": "8.10.0",
    "better-npm-audit": "^3.11.0",
    "eslint": "9.26.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "ts-jest": "29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.54.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johanna-herrmann/web-vault-org/web-vault.git"
  },
  "keywords": [
    "encryption",
    "crypto",
    "web",
    "vault"
  ],
  "author": "Johanna Herrmann",
  "license": "MIT",
  "dependencies": {
    "argon2": "^0.44.0",
    "argon2-browser": "^1.18.0"
  }
}
